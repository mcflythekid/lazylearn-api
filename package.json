{
  "name": "lazylearn-api",
  "version": "2.0.0",
  "description": "API for Lazylearn flashcard learner",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "envigenter": "^1.2.0",
    "hygen": "^4.0.4",
    "hygen-add": "^1.0.5",
    "hygen-envigenter": "^1.0.0"
  },
  "scripts": {
    "_docker:up": "  docker-compose -f docker/default/docker-compose.yml up -d --force-recreate",
    "_docker:down": "docker-compose -f docker/default/docker-compose.yml down || true",
    "test": "echo \"Error: no test specified\" && exit 1",
    "_mvn:install": "rm -rf target && mvn -DskipTests clean install",
    "_stop": "npm run _docker:down",
    "_build": "npm i && npm run _stop && envigenter default && npm run _mvn:install",
    "dev": "envigenter default",
    "start": "npm run _build && npm run _docker:up"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcflythekid/lazylearn-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mcflythekid/lazylearn-api/issues"
  },
  "homepage": "https://github.com/mcflythekid/lazylearn-api#readme"
}
